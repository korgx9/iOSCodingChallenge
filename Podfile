# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'iOSCodingChallenge' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOSCodingChallenge
  pod 'Alamofire', '~> 4.4'
  pod 'ObjectMapper', '~> 2.2.9'
  pod 'AlamofireObjectMapper', '~> 4.1.0'
  pod 'Mapbox-iOS-SDK'
  pod 'MapboxGeocoder.swift', '~> 0.6'
  pod 'DZNEmptyDataSet'

  target 'iOSCodingChallengeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iOSCodingChallengeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
